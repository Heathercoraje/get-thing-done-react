{
  parser: "babel-eslint",
  "plugins":[
    "react",
    "jsx-a11y",
    "import"
  ],
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-unused-vars": 0
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"],
  "globals": {
    "Style": true,
    "document": true,
    "describe": true,
    "it":true,
    "expect":true
  }
}
